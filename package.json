{
	"name": "ezzyspell",
	"version": "1.2.0",
	"private": true,
	"scripts": {
		"dev": "npm run prisma:gen:dev && vite dev",
		"wrangler:dev": "npx wrangler pages dev --local --port 5173 -- vite dev",
		"build": "npm run prisma:gen:prod && vite build --mode production",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma:gen:dev": "npx prisma generate --schema ./prisma/dev.schema.prisma",
		"prisma:gen:prod": "npx prisma generate --no-engine --schema ./prisma/prod.schema.prisma",
		"prisma:push": "npx prisma db push --schema ./prisma/prod.schema.prisma",
		"prisma:studio": "npx prisma studio --schema ./prisma/prod.schema.prisma"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.5.1",
		"@skeletonlabs/skeleton": "^2.6.0",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@tailwindcss/forms": "^0.5.4",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"cloudinary": "^1.41.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.7.0",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.5.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"wrangler": "^3.22.1",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^3.0.0",
		"@lucia-auth/oauth": "^3.5.0",
		"@prisma/client": "^5.7.0",
		"@prisma/extension-accelerate": "^0.6.2",
		"@steeze-ui/heroicons": "^2.2.3",
		"@steeze-ui/svelte-icon": "^1.5.0",
		"@tanstack/svelte-table": "^8.9.5",
		"lucia": "^2.0.0"
	}
}
